<?xml version="1.0" encoding="UTF-8"?>
<printing_document name="receipt" font="Calibri" font_size="12">
    <paragraph text_align="left">
        <line><text font_style="bold" font_size="14">Ficha de Gravação</text></line>
    </paragraph>
    <paragraph text_align="left">
        <line><text>Data: <%= lib.dateTimeString() || "-" %></text></line>
        <line><text>Responsável: <%= config.responsible || "-" %></text></line>
        <line><text>Tecnologia: <%= config.technology || "-" %></text></line>
        <line><text>Elementos: <%= config.elements || "-" %></text></line>
        <line><text>Localização: <%= config.location || "-" %></text></line>
        <line><image source="<%= textImageBase64 || '' %>" /></line>
    </paragraph>
</printing_document>
