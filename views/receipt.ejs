<?xml version="1.0" encoding="UTF-8"?>
<printing_document name="receipt" font="Calibri" font_size="12">
    <paragraph text_align="left">
        <line><text font_style="bold" font_size="14">Engraving File</text></line>
    </paragraph>
    <paragraph text_align="left">
        <line><text>Date: <%= lib.dateTimeString() || "-" %></text></line>
        <line><text>Responsible: <%= config.responsible || "-" %></text></line>
        <line><text>Technology: <%= localize(config.technology || "-") %></text></line>
        <line><text>Elements: <%= localize(config.elements || "-") %></text></line>
        <line><text>Location: <%= localize(config.location || "-") %></text></line>
        <line><image source="<%= textImageBase64 || '' %>" /></line>
    </paragraph>
</printing_document>
